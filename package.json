		"@material-ui/styles": "^5.0.0-alpha.35",
		"clsx": "^1.1.1",
		"critters": "^0.0.10",
		"face-api.js": "^0.22.2",
		"next": "^10.2.3",
		"next-pwa": "^5.2.21",
		"prop-types": "^15.7.2",
