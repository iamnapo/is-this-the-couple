			">1%",
			"not dead",
			"not op_mini all",
			"not ie > 0",
			"not ie_mob > 0"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"prettier": "@iamnapo/prettier-config",
	"eslintConfig": {
		"extends": [
			"iamnapo/with-react"
		],
		"rules": {
			"unicorn/prefer-module": "off"
		},
		"ignorePatterns": [
			".next",
			"netlify",
			"out",
			"public/workbox-*",
			"public/sw*"
		]
	},
	"dependencies": {
		"@emotion/cache": "^11.4.0",
		"@emotion/react": "^11.4.0",
		"@emotion/server": "^11.4.0",
